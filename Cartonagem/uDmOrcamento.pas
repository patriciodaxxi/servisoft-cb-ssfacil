var
  dmOrcamento: TdmOrcamento;

implementation

uses uDmDatabase;

{$R *.dfm}

procedure TdmOrcamento.prc_Inserir;
var
  vAux : Integer;
begin
  if not cdsProduto.Active then
    prc_Localizar(-1);
  vAux := dmDatabase.ProximaSequencia('PRODUTO',0);

  cdsProduto.Insert;
  cdsProdutoID.AsInteger := vAux;
end;

procedure TdmOrcamento.prc_Localizar(ID: Integer);
begin
  cdsProduto.Close;
  sdsProduto.CommandText := ctProduto;
  if ID <> 0 then
    sdsProduto.CommandText := sdsProduto.CommandText + ' WHERE ID = ' + IntToStr(ID);
  cdsProduto.Open;
end;

procedure TdmOrcamento.DataModuleCreate(Sender: TObject);
begin
  ctProduto := sdsProduto.CommandText;
end;

procedure TdmOrcamento.prc_Excluir(ID: Integer);
begin
  prc_Localizar(ID);
  cdsProduto.Delete;
  cdsProduto.ApplyUpdates(0);
end;

end.
