uses uDmDatabase;

{$R *.dfm}

procedure TdmCartoOrcamento.prc_Excluir(ID: Integer);
begin
  prc_Localizar(ID);
  cdsCartoOrcamento.Delete;
  cdsCartoOrcamento.ApplyUpdates(0);
end;

procedure TdmCartoOrcamento.prc_Gravar;
begin
  vMsgErro := '';
  cdsCartoOrcamentoORCAMENTO_ID.AsInteger := cdsCartoOrcamentoID.AsInteger;
  if trim(cdsCartoOrcamentoCLIENTE_NOME.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Cliente não informado!';
  if trim(cdsCartoOrcamentoACABADO_NOME.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Produto (acabado) não informado!';

  if vMsgErro <> '' then
    Exit;

  cdsCartoOrcamento.Post;
  cdsCartoOrcamento.ApplyUpdates(0);
end;

procedure TdmCartoOrcamento.prc_Inserir;
var
  vAux: Integer;
begin
  if not cdsCartoOrcamento.Active then
    prc_Localizar(-1);
  vAux := dmDatabase.ProximaSequencia('CARTO_ORCAMENTO',0);

  cdsCartoOrcamento.Insert;
  cdsCartoOrcamentoID.AsInteger    := vAux;
  cdsCartoOrcamentoDATA.AsDateTime := Date;
  cdsCartoOrcamentoQTD1.AsInteger  := 1;
  cdsCartoOrcamentoQTD2.AsInteger  := 1;
  cdsCartoOrcamentoQTD3.AsInteger  := 1;
end;

procedure TdmCartoOrcamento.prc_Localizar(ID: Integer);
begin
  cdsCartoOrcamento.Close;
  sdsCartoOrcamento.CommandText := ctOrcamento;
  if ID <> 0 then
    sdsCartoOrcamento.CommandText := sdsCartoOrcamento.CommandText + ' WHERE ID = ' + IntToStr(ID);
  cdsCartoOrcamento.Open;
end;

procedure TdmCartoOrcamento.prc_Inserir_Item;
var
  vItem: Integer;
begin
  cdsCartoOrcamentoItem.Last;
  vItem := cdsCartoOrcamentoItemITEM.AsInteger;
  cdsCartoOrcamentoItem.Insert;
  cdsCartoOrcamentoItemORCAMENTO_ID.AsInteger := cdsCartoOrcamentoID.AsInteger;
  cdsCartoOrcamentoItemITEM.AsInteger         := vItem + 1;
end;

end.
